/*C program to insert an element in an array*/
#include <stdio.h>
#include <conio.h>
void main()
{
   int array[50], c, n;
   printf("Enter number of elements in array\n");
   scanf("%d", &n);
 
   printf("Enter %d elements\n", n);
 
   for (c = 0; c < n; c++)
      scanf("%d", &array[c]);
 
   printf("Resul array is\n");
 
   for (c = 0; c <n; c++)
      printf("%d\n", array[c]);
 
   getch();
}
